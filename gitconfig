[user]
	name = Shawn Sorichetti
	email = ssoriche@gmail.com
[core]
	excludesfile = /Users/ssoriche/.gitignore
	ignorecase = false
        editor = /usr/local/Cellar/macvim/v7.3-53/MacVim.app/Contents/MacOS/Vim
[color]
	status = auto
	diff = auto
	branch = auto
[branch "master"]
	merge = master
[alias]
  ci = commit -a
  cl = clean -dfx
  cc = cherry-pick
  fx = commit -a --amend -C HEAD
  st = status -sb
  hist = log --all --graph --pretty='[%C(cyan)%h%Creset]%C(bold cyan)%d%Creset %s'
  uncommit = reset --soft HEAD^
  unstage = reset HEAD
  standup = shortlog --since=yesterday
[push]
	default = current
